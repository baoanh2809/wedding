                        each story,i in storys
                            if(i<1)
                                .col.ms-lg-n4.mb-4.mb-lg-0.box-header
                                    h2.fw-bold.mb-0 Wedding site!
                                    p.fs-5.text-gray-700.mb-0.limit-1-line
                                        svg.bi.bi-link-45deg(xmlns='http://www.w3.org/2000/svg' width='16' height='16' fill='currentColor' viewbox='0 0 16 16')
                                            path(d='M4.715 6.542 3.343 7.914a3 3 0 1 0 4.243 4.243l1.828-1.829A3 3 0 0 0 8.586 5.5L8 6.086a1.002 1.002 0 0 0-.154.199 2 2 0 0 1 .861 3.337L6.88 11.45a2 2 0 1 1-2.83-2.83l.793-.792a4.018 4.018 0 0 1-.128-1.287z')
                                            path(d='M6.586 4.672A3 3 0 0 0 7.414 9.5l.775-.776a2 2 0 0 1-.896-3.346L9.12 3.55a2 2 0 1 1 2.83 2.83l-.793.792c.112.42.155.855.128 1.287l1.372-1.372a3 3 0 1 0-4.243-4.243L6.586 4.672z')
                                        span.d-inline.d-lg-none
                                            a(href=`/wedding/preview/${story.wedding}` target='_blank') Link website
                                        span.d-none.d-lg-inline
                                            a.text-gray-700(href=`/wedding/preview/${story.wedding}` target='_blank') Nhấp Vào Đây Để Xem Website Của Bạn!
                                .col-12.col-lg-auto.box-header
                                    button.btn.btn-sm.btn-danger-soft.btn-remove-website(data-id=`${story.wedding}` type='submit' )
                                        span.fe.fe-trash-2
                                        |  X&oacute;a website
            if(storys)
                each story,i in storys
                    if(i<1)
                        ul.nav.nav-pills.nav-fill.nav-list-website-menu
                            li.nav-item
                                a.nav-link.bg-gray-200(aria-current='page' href=`/wedding/edit/${story.wedding}/bridegroom`) C&ocirc; D&acirc;u &amp; Ch&uacute; R&#x1EC3;
                            li.nav-item
                                a.nav-link.bg-gray-200(href=`/wedding/edit/${story.wedding}/bridesmaids`) Ph&ugrave; D&acirc;u &amp; Ph&ugrave; R&#x1EC3;
                            li.nav-item
                                a.nav-link.bg-gray-200(href=`/wedding/edit/${story.wedding}/event`) S&#x1EF1; ki&#x1EC7;n
                            li.nav-item
                                a.nav-link.active(href=`/wedding/edit/${story.wedding}/storylove`) Chuy&#x1EC7;n T&igrave;nh Y&ecirc;u
                            li.nav-item
                                a.nav-link.bg-gray-200(href=`/wedding/edit/${story.wedding}/contact`) Lời Chúc
                            li.nav-item
                                a.nav-link.bg-gray-200(href=`/wedding/edit/${story.wedding}/templatesEdit`) Giao Diện 
            input(type='hidden' name='_csrf-biihappy' value='LpfGO0r-Dc-bpvWJ1J993GvVrllhRJn3BLXu-H93gUd2rqcDLpd_tvbRl-a61A6lCZ3kLiQC1oJg27abCxazNA==')
            .storiesContainer.row.mt-6
                each story,i in storys  
                    .row-story.col-sm-6.col-md-4.col-lg-3
                        .card.card-border.border-dark.shadow-lg.card-border-radius-none.mb-7
                        form#stories-form.form-story-user                  
                            .card-body
                                a.thumbnail.d-block.mb-3.choose-image-parent.position-relative(href='javascript:void(0)')
                                    .choose-image-overlay.d-flex.justify-content-center.align-items-center.position-absolute.top-0.start-0.w-100.h-100
                                        button.btn.btn-primary.btn-xs.position-relative(type='button')
                                            | Thay &dstrok;&#x1ED5;i
                                        input.upload_image_event.file-field-absolute(type='file' accept='image/*' id =`storyPhoto${i+1}` name=`storyPhoto${i+1}`)
                                        input.form-control(type='hidden' id=`storyphotostore${i+1}` name=`storyphotostore${i+1}` value=`${story.storyPhoto}`)
                                    img#show_image_event.img-thumbnail.w-100.img-photo.rounded-0(src=`/img/wedding/${story.storyPhoto}`)
                                //- .field-story_photo
                                //-     input#story_photo.form-control(type='hidden' name='WebsiteInformationStoryForm[photo][]' value='default/website/383875f7dbad0c73b240243aeecb74dd.jpeg')
                                //-     .help-block
                                .field-story_title.required
                                    .field-story-id.required
                                        input.form-control(type='hidden' name='id' id=`id${i+1}` value=`${i+1}`)
                                        .help-block
                                    .form-floating.mb-2
                                        input.form-control(type='text' name=`title${i+1}` id=`title${i+1}` value=`${story.title}` placeholder='Tiêu đề')
                                        label.control-label(for='title') Ti&ecirc;u &dstrok;&#x1EC1;
                                        .help-block
                                .field-story_time.required
                                    .form-floating.mb-2
                                        input.form-control(type='date' name=`time${i+1}` id=`time${i+1}` value=`${story.time}` placeholder='Thời gian')
                                        label.control-label(for='time') Th&#x1EDD;i gian
                                    .help-block
                                .field-story_intro.required
                                    .form-floating.mb-2
                                        textarea.form-control(name=`content${i+1}` id=`content${i+1}` style='height:150px;resize: none;' placeholder='Nội dung')=`${story.content}`
                                        label.control-label(for='content') N&#x1ED9;i dung
                                        .help-block
                                    .form-floating.mb-2
                                        input.form-control(type='hidden' id=`storyId${i+1}` name=`storyId${i+1}` value=`${story.id}`)
                                button.btn.btn-danger.me-7(type='submit' style="margin-right:25px;height:55px")
                                    svg.bi.bi-save(xmlns='http://www.w3.org/2000/svg' width='16' height='16' fill='currentColor' viewbox='0 0 16 16')
                                        path(d='M2 1a1 1 0 0 0-1 1v12a1 1 0 0 0 1 1h12a1 1 0 0 0 1-1V2a1 1 0 0 0-1-1H9.5a1 1 0 0 0-1 1v7.293l2.646-2.647a.5.5 0 0 1 .708.708l-3.5 3.5a.5.5 0 0 1-.708 0l-3.5-3.5a.5.5 0 1 1 .708-.708L7.5 9.293V2a2 2 0 0 1 2-2H14a2 2 0 0 1 2 2v12a2 2 0 0 1-2 2H2a2 2 0 0 1-2-2V2a2 2 0 0 1 2-2h2.5a.5.5 0 0 1 0 1H2z')
                                    | &#x9;&#x9;L&#x1B0;u 
                                button.btn.btn-link.btn-delete-event.btn-danger-soft.btn-delete-story(data-id=`${story.id}`) X&oacute;a
            each story,i in storys
                if(i<1)       
                    .p-5.text-center.bg-gray-200
                        button.btn.btn-secondary.button-add-more-story#add-more-story(data-story=`${story.wedding}`)
                            svg.bi.bi-plus(xmlns='http://www.w3.org/2000/svg' width='16' height='16' fill='currentColor' viewbox='0 0 16 16')
                                path(d='M8 4a.5.5 0 0 1 .5.5v3h3a.5.5 0 0 1 0 1h-3v3a.5.5 0 0 1-1 0v-3h-3a.5.5 0 0 1 0-1h3v-3A.5.5 0 0 1 8 4z')
                            | &#x9;&#x9;Th&ecirc;m s&#x1EF1; ki&#x1EC7;n
